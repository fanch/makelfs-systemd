# Description: The GNU C++ lib 
# URL: http://gcc.gnu.org
# Maintainer: Jakub Jelinek jakub at redhat dot com
PKGMK_GROUPS=()
source=(\
http://ftp.gnu.org/gnu/gcc/gcc-$version/gcc-$version.tar.bz2 )

build()
{
cd  gcc-$version
mkdir -pv ../gcc-build
cd ../gcc-build

../gcc-$version/libstdc++-v3/configure --prefix=/
make
make DESTDIR=$PKG install
rm -r $PKG/{include,share}
rm -f $PKG/lib{,64}/libstdc++.so.6.0.18-gdb.py

}
