source=(http://prdownloads.sourceforge.net/psmisc/psmisc-$version.tar.gz)
build()
{
cd psmisc-$version
./configure --prefix=/usr --libdir=/lib --bindir=/bin
make
make DESTDIR=$PKG install
}
